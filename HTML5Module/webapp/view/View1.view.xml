                                    <Label text="{i18n>custId}" id="label9"/>
                                </header>
                            </Column>
                            <Column id="column10" >
                                <header>
                                    <Label text="{i18n>coName}" id="label10"/>
                                </header>
                            </Column>
                            <Column id="column11" minScreenWidth="Tablet" demandPopin="true">
                                <header>
                                    <Label text="{i18n>Addr}" id="label11"/>
                                </header>
                            </Column>
                             <Column id="column12" minScreenWidth="Tablet" demandPopin="true">
                                <header>
                                    <Label text="{i18n>city}" id="label12"/>
                                </header>
                            </Column>
                            </columns></Table>
                    </content>
				</Page>
			</pages>
		</App>
