                                    <Text text="{CustomerID}" id="text48"/>
                                    <Text text="{CompanyName}" id="text49"/>
                                    <Text text="{Address}" id="text50"/>
                                    <Text text="{City}" id="text51"/></cells>
                            </ColumnListItem>
                        </items>
                        <columns>
                            <Column id="column9">
